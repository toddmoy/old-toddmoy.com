<!doctype html>
<html itemscope itemtype="http://schema.org/Article">
  <head>
    <title><%= current_page.data.page_title || 'Todd Moy' %></title>
    <link rel="icon" type="image/png" href="<%= @site_favicon_image %>" />
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= stylesheet_link_tag "all" %>
    <%= javascript_include_tag  "all" %>
    <%= partial "partials/seo/head-meta" %>
  </head>

  <body id="main" class="<%= page_classes %>">
    <%= partial "partials/components/header" %>
    <%= yield %>
    <%= partial "partials/tracking/ga" %>
  </body>
</html>
